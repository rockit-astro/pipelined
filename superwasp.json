{
  "daemon": "superwasp_pipeline",
  "log_name": "pipelined@superwasp",
  "control_machines": ["LocalHost", "SWASPTCS"],
  "notify_frame_machines": ["SWASPCameraPi1", "SWASPCameraPi2", "SWASPCameraPi3", "SWASPCameraPi4"],
  "environment_daemon": "observatory_environment",
  "environment_query_timeout": 1,
  "telescope_query_timeout": 1,
  "ops_daemon": "superwasp_operations",
  "guiding_min_interval": 5,
  "cameras": {
    "CAM1": {
      "worker_daemon": "superwasp_pipeline_cam1",
      "worker_processes": 2,
      "input_data_path": "/data/incoming",
      "output_data_path": "/data",
      "dashboard_output_path": "/mnt/dashboard-generated",
      "dashboard_prefix": "dashboard-1",
      "wcs_scale_high": 4.5,
      "wcs_scale_low": 3.5,
      "wcs_timeout": 10.0,
      "wcs_search_ra_card": "TELRA",
      "wcs_search_dec_card": "TELDEC",
      "wcs_search_radius": 10,
      "ccd_bin_card": "CCD-XBIN",
      "image_region_card": "IMAG-RGN",
      "platescale": 3.98,
      "object_minpix": 16,
      "preview_min_interval": 5,
      "preview_max_instances": 5,
      "preview_ds9_width": 810,
      "preview_ds9_height": 580,
      "preview_ds9_zoom": 0.2,
      "preview_ds9_annotation_margin": 175,
      "dashboard_flip_vertical": true,
      "dashboard_flip_horizontal": false,
      "dashboard_min_threshold": 5,
      "dashboard_max_threshold": 95,
      "dashboard_thumb_size": 1024,
      "dashboard_clip_size": 1024,
      "dashboard_min_interval": 30,
      "hfd_grid_tiles_x": 5,
      "hfd_grid_tiles_y": 4
    },
    "CAM2": {
      "worker_daemon": "superwasp_pipeline_cam2",
      "worker_processes": 2,
      "input_data_path": "/data/incoming",
      "output_data_path": "/data",
      "dashboard_output_path": "/mnt/dashboard-generated",
      "dashboard_prefix": "dashboard-2",
      "wcs_scale_high": 4.5,
      "wcs_scale_low": 3.5,
      "wcs_timeout": 10.0,
      "wcs_search_ra_card": "TELRA",
      "wcs_search_dec_card": "TELDEC",
      "wcs_search_radius": 10,
      "ccd_bin_card": "CCD-XBIN",
      "image_region_card": "IMAG-RGN",
      "platescale": 3.98,
      "object_minpix": 16,
      "preview_min_interval": 5,
      "preview_max_instances": 5,
      "preview_ds9_width": 810,
      "preview_ds9_height": 580,
      "preview_ds9_zoom": 0.2,
      "preview_ds9_annotation_margin": 175,
      "dashboard_flip_vertical": true,
      "dashboard_flip_horizontal": false,
      "dashboard_min_threshold": 5,
      "dashboard_max_threshold": 95,
      "dashboard_thumb_size": 1024,
      "dashboard_clip_size": 1024,
      "dashboard_min_interval": 30,
      "hfd_grid_tiles_x": 5,
      "hfd_grid_tiles_y": 4
    },
    "CAM3": {
      "worker_daemon": "superwasp_pipeline_cam3",
      "worker_processes": 2,
      "input_data_path": "/data/incoming",
      "output_data_path": "/data",
      "dashboard_output_path": "/mnt/dashboard-generated",
      "dashboard_prefix": "dashboard-3",
      "wcs_scale_high": 4.5,
      "wcs_scale_low": 3.5,
      "wcs_timeout": 10.0,
      "wcs_search_ra_card": "TELRA",
      "wcs_search_dec_card": "TELDEC",
      "wcs_search_radius": 10,
      "ccd_bin_card": "CCD-XBIN",
      "image_region_card": "IMAG-RGN",
      "platescale": 3.98,
      "object_minpix": 16,
      "preview_min_interval": 5,
      "preview_max_instances": 5,
      "preview_ds9_width": 810,
      "preview_ds9_height": 580,
      "preview_ds9_zoom": 0.2,
      "preview_ds9_annotation_margin": 175,
      "dashboard_flip_vertical": true,
      "dashboard_flip_horizontal": false,
      "dashboard_min_threshold": 5,
      "dashboard_max_threshold": 95,
      "dashboard_thumb_size": 1024,
      "dashboard_clip_size": 1024,
      "dashboard_min_interval": 30,
      "hfd_grid_tiles_x": 5,
      "hfd_grid_tiles_y": 4
    },
    "CAM4": {
      "worker_daemon": "superwasp_pipeline_cam4",
      "worker_processes": 2,
      "input_data_path": "/data/incoming",
      "output_data_path": "/data",
      "dashboard_output_path": "/mnt/dashboard-generated",
      "dashboard_prefix": "dashboard-4",
      "wcs_scale_high": 4.5,
      "wcs_scale_low": 3.5,
      "wcs_timeout": 10.0,
      "wcs_search_ra_card": "TELRA",
      "wcs_search_dec_card": "TELDEC",
      "wcs_search_radius": 10,
      "ccd_bin_card": "CCD-XBIN",
      "image_region_card": "IMAG-RGN",
      "platescale": 3.98,
      "object_minpix": 16,
      "preview_min_interval": 5,
      "preview_max_instances": 5,
      "preview_ds9_width": 810,
      "preview_ds9_height": 580,
      "preview_ds9_zoom": 0.2,
      "preview_ds9_annotation_margin": 175,
      "dashboard_flip_vertical": true,
      "dashboard_flip_horizontal": false,
      "dashboard_min_threshold": 5,
      "dashboard_max_threshold": 95,
      "dashboard_thumb_size": 1024,
      "dashboard_clip_size": 1024,
      "dashboard_min_interval": 30,
      "hfd_grid_tiles_x": 5,
      "hfd_grid_tiles_y": 4
    }
  },
  "environment_cards": [
    {
      "key": "EXTTEMP",
      "comment": "[deg c] temperature outside dome",
      "sensor": "goto_vaisala",
      "parameter": "temperature",
      "type": "float1dp"
    },
    {
      "key": "EXTHUMD",
      "comment": "[%] humidity outside dome",
      "sensor": "goto_vaisala",
      "parameter": "relative_humidity",
      "type": "float1dp"
    },
    {
      "key": "PRESSURE",
      "comment": "[hPa] air pressure",
      "sensor": "goto_vaisala",
      "parameter": "pressure",
      "type": "float1dp"
    },
    {
      "key": "WINDSPD",
      "comment": "[m/s] wind speed outside dome",
      "sensor": "goto_vaisala",
      "parameter": "wind_speed",
      "type": "float1dp"
    },
    {
      "key": "WINDGUST",
      "comment": "[m/s] wind gust speed outside dome",
      "sensor": "goto_vaisala",
      "parameter": "wind_gust",
      "type": "float1dp"
    },
    {
      "key": "MEDWIND",
      "comment": "[m/s] median wind speed (last 20 min)",
      "sensor": "goto_vaisala",
      "parameter": "median_wind_speed",
      "type": "float1dp"
    },
    {
      "key": "DOMETEMP",
      "comment": "[deg c] temperature inside telescope room",
      "sensor": "superwasp_roomalert",
      "parameter": "cam_room_temp",
      "type": "float1dp"
    },
    {
      "key": "DOMEHUMD",
      "comment": "[%] humidity inside telescope room",
      "sensor": "superwasp_roomalert",
      "parameter": "cam_room_humidity",
      "type": "float1dp"
    },
    {
      "key": "SKYCLAR",
      "comment": "[deg c] sky clarity",
      "sensor": "superwasp_aurora",
      "parameter": "clarity",
      "type": "float1dp"
    },
    {
      "key": "DEWDELTA",
      "comment": "[deg c] temperature above dew point",
      "sensor": "goto_vaisala",
      "parameter": "dew_point_delta",
      "type": "float1dp"
    },
    {
      "key": "TNGDIMM",
      "comment": "[arcsec] TNG DIMM seeing measurement",
      "sensor": "tng",
      "parameter": "seeing",
      "type": "float2dp"
    },
    {
      "key": "INGDIMM",
      "comment": "[arcsec] ING RoboDIMM seeing measurement",
      "sensor": "robodimm",
      "parameter": "seeing",
      "type": "float2dp"
    },
    {
      "key": "TNGDUST",
      "comment": "[ug/m3] TNG Dust measurement",
      "sensor": "tng",
      "parameter": "dust",
      "type": "float1dp"
    }
  ],
  "telescope_cards": [
    {
      "key": "TELSWVER",
      "comment": "tcs server software version",
      "type": "string",
      "daemon": "superwasp_telescope",
      "method": "report_status",
      "parameter": "software_version"
    },
    {
      "key": "TELSTATE",
      "comment": "telescope status",
      "type": "string",
      "daemon": "superwasp_telescope",
      "method": "report_status",
      "parameter": "state_label"
    },
    {
      "key": "ROOFSTAT",
      "comment": "roof status",
      "type": "string",
      "daemon": "superwasp_telescope",
      "method": "report_status",
      "parameter": "roof_state_label"
    },
    {
      "key": "TELRA",
      "comment": "telescope nominal J2000 RA",
      "type": "sexagesimalha",
      "daemon": "superwasp_telescope",
      "method": "report_status",
      "parameter": "ra"
    },
    {
      "key": "TELDEC",
      "comment": "telescope nominal J2000 Dec",
      "type": "sexagesimaldeg",
      "daemon": "superwasp_telescope",
      "method": "report_status",
      "parameter": "dec"
    },
    {
      "key": "TELHA",
      "comment": "telescope nominal HA",
      "type": "sexagesimalha",
      "daemon": "superwasp_telescope",
      "method": "report_status",
      "parameter": "ha"
    },
    {
      "key": "TELRAD",
      "comment": "[deg] telescope nominal J2000 RA",
      "type": "float5dp",
      "daemon": "superwasp_telescope",
      "method": "report_status",
      "parameter": "ra"
    },
    {
      "key": "TELDECD",
      "comment": "[deg] telescope nominal J2000 Dec",
      "type": "float5dp",
      "daemon": "superwasp_telescope",
      "method": "report_status",
      "parameter": "dec"
    },
    {
      "key": "TELHAD",
      "comment": "[deg] telescope nominal hour angle",
      "type": "float5dp",
      "daemon": "superwasp_telescope",
      "method": "report_status",
      "parameter": "ha"
    },
    {
      "key": "ALTITUDE",
      "comment": "[deg] telescope nominal altitude",
      "type": "float5dp",
      "daemon": "superwasp_telescope",
      "method": "report_status",
      "parameter": "alt"
    },
    {
      "key": "AZIMUTH",
      "comment": "[deg] telescope nominal azimuth",
      "type": "float5dp",
      "daemon": "superwasp_telescope",
      "method": "report_status",
      "parameter": "az"
    },
    {
      "key": "SITELAT",
      "comment": "telescope latitude (north)",
      "type": "sexagesimaldeg",
      "daemon": "superwasp_telescope",
      "method": "report_status",
      "parameter": "site_latitude"
    },
    {
      "key": "SITELONG",
      "comment": "telescope longitude (east)",
      "type": "sexagesimaldeg",
      "daemon": "superwasp_telescope",
      "method": "report_status",
      "parameter": "site_longitude"
    },
    {
      "key": "SITEELEV",
      "comment": "[m] telescope elevation",
      "type": "int",
      "daemon": "superwasp_telescope",
      "method": "report_status",
      "parameter": "site_elevation"
    },
    {
      "key": "MOONSEP",
      "comment": "[deg] nominal distance from moon",
      "type": "float1dp",
      "daemon": "superwasp_telescope",
      "method": "report_status",
      "parameter": "moon_separation"
    },
    {
      "key": "SUNSEP",
      "comment": "[deg] nominal distance from sun",
      "type": "float1dp",
      "daemon": "superwasp_telescope",
      "method": "report_status",
      "parameter": "sun_separation"
    },
    {
      "key": "LENSTEMP",
      "comment": "[deg c] lens temperature",
      "camera": "CAM1",
      "daemon": "superwasp_lensheater",
      "method": "report_status",
      "parameter": "temp_1",
      "type": "float1dp"
    },
    {
      "key": "LENSSET",
      "comment": "[deg c] lens temperature set point",
      "camera": "CAM1",
      "daemon": "superwasp_lensheater",
      "method": "report_status",
      "parameter": "set_1",
      "type": "float1dp"
    },
    {
      "key": "LENSTEMP",
      "comment": "[deg c] lens temperature",
      "camera": "CAM2",
      "daemon": "superwasp_lensheater",
      "method": "report_status",
      "parameter": "temp_2",
      "type": "float1dp"
    },
    {
      "key": "LENSSET",
      "comment": "[deg c] lens temperature set point",
      "camera": "CAM2",
      "daemon": "superwasp_lensheater",
      "method": "report_status",
      "parameter": "set_2",
      "type": "float1dp"
    },
    {
      "key": "LENSTEMP",
      "comment": "[deg c] lens temperature",
      "camera": "CAM3",
      "daemon": "superwasp_lensheater",
      "method": "report_status",
      "parameter": "temp_3",
      "type": "float1dp"
    },
    {
      "key": "LENSSET",
      "comment": "[deg c] lens temperature set point",
      "camera": "CAM3",
      "daemon": "superwasp_lensheater",
      "method": "report_status",
      "parameter": "set_3",
      "type": "float1dp"
    },
    {
      "key": "LENSTEMP",
      "comment": "[deg c] lens temperature",
      "camera": "CAM4",
      "daemon": "superwasp_lensheater",
      "method": "report_status",
      "parameter": "temp_4",
      "type": "float1dp"
    },
    {
      "key": "LENSSET",
      "comment": "[deg c] lens temperature set point",
      "camera": "CAM4",
      "daemon": "superwasp_lensheater",
      "method": "report_status",
      "parameter": "set_4",
      "type": "float1dp"
    }
  ]
}
