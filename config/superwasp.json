{
  "daemon": "superwasp_pipeline",
  "log_name": "pipelined@superwasp",
  "control_machines": ["SWASPTCS", "SWASPDAS1", "SWASPDAS2"],
  "notify_frame_machines": ["SWASPCameraVM1", "SWASPCameraVM2", "SWASPCameraVM3", "SWASPCameraVM4"],
  "environment_daemon": "observatory_environment",
  "environment_query_timeout": 1,
  "telescope_query_timeout": 1,
  "ops_daemon": "superwasp_operations",
  "guiding_min_interval": 5,
  "ssh_root_path": "/home/ops/.ssh",
  "preview_clients": [
    {
        "remote_machine": "SWASPTCS",
        "remote_user": "ops",
        "remote_path": "/tmp/",
        "remote_command": "/usr/bin/pipeline display",
        "key": "id_rsa"
    },
    {
        "remote_machine": "SWASPDAS1",
        "remote_user": "ops",
        "remote_path": "/tmp/",
        "remote_command": "/usr/bin/pipeline display",
        "key": "id_rsa"
    },
    {
        "remote_machine": "SWASPDAS2",
        "remote_user": "ops",
        "remote_path": "/tmp/",
        "remote_command": "/usr/bin/pipeline display",
        "key": "id_rsa"
      }
  ],
  "cameras": {
    "CAM1": {
      "worker_daemon": "superwasp_pipeline_cam1",
      "worker_processes": 2,
      "input_data_path": "/data/incoming",
      "output_data_path": "/data/raw",
      "output_filename_add_camera_id": true,
      "output_filename_add_filter": false,
      "binning_card": "CAM-BIN",
      "image_region_card": "IMAG-RGN",
      "overscan_region_card": "BIAS-RGN",
      "platescale": 3.99,
      "object_minpix": 16,
      "preview_min_interval": 5,
      "preview_max_instances": 5,
      "preview_ds9_width": 810,
      "preview_ds9_height": 580,
      "preview_ds9_zoom": 0.2,
      "preview_ds9_annotation_margin": 175,
      "preview_display_camera_id": true,
      "preview_display_filter": false,
      "hfd_grid_tiles_x": 5,
      "hfd_grid_tiles_y": 4,
      "wcs": {
        "astrometry_daemon": "localhost_test",
        "scale_high": 4.5,
        "scale_low": 3.5,
        "timeout": 10.0,
        "tel_ra_card": "TELRA",
        "tel_dec_card": "TELDEC",
        "search_radius": 10
      },
      "dashboard": {
        "key": "dashboard",
        "remote_machine": "GOTOServer",
        "remote_user": "dashboarddata",
        "remote_path": "/srv/dashboard/generated",
        "prefix": "dashboard-1",
        "flip_vertical": true,
        "flip_horizontal": false,
        "min_threshold": 5,
        "max_threshold": 95,
        "thumb_size": 1024,
        "clip_size": 1024,
        "min_interval": 30
      }
    },
    "CAM2": {
      "worker_daemon": "superwasp_pipeline_cam2",
      "worker_processes": 2,
      "input_data_path": "/data/incoming",
      "output_data_path": "/data/raw",
      "output_filename_add_camera_id": true,
      "output_filename_add_filter": false,
      "binning_card": "CAM-BIN",
      "image_region_card": "IMAG-RGN",
      "overscan_region_card": "BIAS-RGN",
      "platescale": 3.99,
      "object_minpix": 16,
      "preview_min_interval": 5,
      "preview_max_instances": 5,
      "preview_ds9_width": 810,
      "preview_ds9_height": 580,
      "preview_ds9_zoom": 0.2,
      "preview_ds9_annotation_margin": 175,
      "preview_display_camera_id": true,
      "preview_display_filter": false,
      "hfd_grid_tiles_x": 5,
      "hfd_grid_tiles_y": 4,
      "wcs": {
        "scale_high": 4.5,
        "scale_low": 3.5,
        "timeout": 10.0,
        "tel_ra_card": "TELRA",
        "tel_dec_card": "TELDEC",
        "search_radius": 10
      },
      "dashboard": {
        "key": "dashboard",
        "remote_machine": "GOTOServer",
        "remote_user": "dashboarddata",
        "remote_path": "/srv/dashboard/generated",
        "prefix": "dashboard-2",
        "flip_vertical": true,
        "flip_horizontal": false,
        "min_threshold": 5,
        "max_threshold": 95,
        "thumb_size": 1024,
        "clip_size": 1024,
        "min_interval": 30
      }
    },
    "CAM3": {
      "worker_daemon": "superwasp_pipeline_cam3",
      "worker_processes": 2,
      "input_data_path": "/data/incoming",
      "output_data_path": "/data/raw",
      "output_filename_add_camera_id": true,
      "output_filename_add_filter": false,
      "binning_card": "CAM-BIN",
      "image_region_card": "IMAG-RGN",
      "overscan_region_card": "BIAS-RGN",
      "platescale": 3.99,
      "object_minpix": 16,
      "preview_min_interval": 5,
      "preview_max_instances": 5,
      "preview_ds9_width": 810,
      "preview_ds9_height": 580,
      "preview_ds9_zoom": 0.2,
      "preview_ds9_annotation_margin": 175,
      "preview_display_camera_id": true,
      "preview_display_filter": false,
      "hfd_grid_tiles_x": 5,
      "hfd_grid_tiles_y": 4,
      "wcs": {
        "scale_high": 4.5,
        "scale_low": 3.5,
        "timeout": 10.0,
        "tel_ra_card": "TELRA",
        "tel_dec_card": "TELDEC",
        "search_radius": 10
      },
      "dashboard": {
        "key": "dashboard",
        "remote_machine": "GOTOServer",
        "remote_user": "dashboarddata",
        "remote_path": "/srv/dashboard/generated",
        "prefix": "dashboard-3",
        "flip_vertical": true,
        "flip_horizontal": false,
        "min_threshold": 5,
        "max_threshold": 95,
        "thumb_size": 1024,
        "clip_size": 1024,
        "min_interval": 30
      }
    },
    "CAM4": {
      "worker_daemon": "superwasp_pipeline_cam4",
      "worker_processes": 2,
      "input_data_path": "/data/incoming",
      "output_data_path": "/data/raw",
      "output_filename_add_camera_id": true,
      "output_filename_add_filter": false,
      "binning_card": "CAM-BIN",
      "image_region_card": "IMAG-RGN",
      "overscan_region_card": "BIAS-RGN",
      "platescale": 3.99,
      "object_minpix": 16,
      "preview_min_interval": 5,
      "preview_max_instances": 5,
      "preview_ds9_width": 810,
      "preview_ds9_height": 580,
      "preview_ds9_zoom": 0.2,
      "preview_ds9_annotation_margin": 175,
      "preview_display_camera_id": true,
      "preview_display_filter": false,
      "hfd_grid_tiles_x": 5,
      "hfd_grid_tiles_y": 4,
      "wcs": {
        "scale_high": 4.5,
        "scale_low": 3.5,
        "timeout": 10.0,
        "tel_ra_card": "TELRA",
        "tel_dec_card": "TELDEC",
        "search_radius": 10
      },
      "dashboard": {
        "key": "dashboard",
        "remote_machine": "GOTOServer",
        "remote_user": "dashboarddata",
        "remote_path": "/srv/dashboard/generated",
        "prefix": "dashboard-4",
        "flip_vertical": true,
        "flip_horizontal": false,
        "min_threshold": 5,
        "max_threshold": 95,
        "thumb_size": 1024,
        "clip_size": 1024,
        "min_interval": 30
      }
    }
  },
  "environment_cards": [
    {
      "key": "EXTTEMP",
      "comment": "[deg c] temperature outside dome",
      "sensor": "w1m_vaisala",
      "parameter": "temperature",
      "type": "float1dp"
    },
    {
      "key": "EXTHUMD",
      "comment": "[%] humidity outside dome",
      "sensor": "w1m_vaisala",
      "parameter": "relative_humidity",
      "type": "float1dp"
    },
    {
      "key": "PRESSURE",
      "comment": "[hPa] air pressure",
      "sensor": "w1m_vaisala",
      "parameter": "pressure",
      "type": "float1dp"
    },
    {
      "key": "WINDSPD",
      "comment": "[m/s] wind speed outside dome",
      "sensor": "w1m_vaisala",
      "parameter": "wind_speed",
      "type": "float1dp"
    },
    {
      "key": "WINDGUST",
      "comment": "[m/s] wind gust speed outside dome",
      "sensor": "w1m_vaisala",
      "parameter": "wind_gust",
      "type": "float1dp"
    },
    {
      "key": "MEDWIND",
      "comment": "[m/s] median wind speed (last 20 min)",
      "sensor": "w1m_vaisala",
      "parameter": "median_wind_speed",
      "type": "float1dp"
    },
    {
      "key": "DOMETEMP",
      "comment": "[deg c] temperature inside dome",
      "sensor": "clasp_domealert",
      "parameter": "superwasp_temperature",
      "type": "float1dp"
    },
    {
      "key": "DOMEHUMD",
      "comment": "[%] humidity inside dome",
      "sensor": "clasp_domealert",
      "parameter": "superwasp_humidity",
      "type": "float1dp"
    },
    {
      "key": "DEWDELTA",
      "comment": "[deg c] temperature above dew point",
      "sensor": "w1m_vaisala",
      "parameter": "dew_point_delta",
      "type": "float1dp"
    },
    {
      "key": "SKYTEMP",
      "comment": "[deg c] sky temperature",
      "sensor": "cloudwatcher",
      "parameter": "sky_temp",
      "type": "float1dp"
    },
    {
      "key": "TNGDIMM",
      "comment": "[arcsec] TNG DIMM seeing measurement",
      "sensor": "tng",
      "parameter": "seeing",
      "type": "float2dp"
    },
    {
      "key": "TNGDUST",
      "comment": "[ug/m3] TNG dust measurement",
      "sensor": "tng",
      "parameter": "dust",
      "type": "float1dp"
    },
    {
      "key": "SUNALT",
      "comment": "[deg] sun altitude",
      "sensor": "ephem",
      "parameter": "sun_alt",
      "type": "float1dp"
    },
    {
      "key": "MOONALT",
      "comment": "[deg] moon altitude",
      "sensor": "ephem",
      "parameter": "moon_alt",
      "type": "float1dp"
    },
    {
      "key": "MOONPCT",
      "comment": "[%] moon illumination",
      "sensor": "ephem",
      "parameter": "moon_percent_illumination",
      "type": "float1dp"
    }
  ],
  "telescope_cards": [
    {
      "key": "MNTSTATE",
      "comment": "telescope status",
      "type": "string",
      "daemon": "superwasp_telescope",
      "method": "report_status",
      "parameter": "state_label"
    },
    {
      "key": "DOMENORT",
      "comment": "dome north shutter status",
      "type": "string",
      "daemon": "superwasp_dome",
      "method": "status",
      "parameter": "shutter_b_label"
    },
    {
      "key": "DOMESOUT",
      "comment": "dome south shutter status",
      "type": "string",
      "daemon": "superwasp_dome",
      "method": "status",
      "parameter": "shutter_a_label"
    },
    {
      "key": "MNTRA",
      "comment": "mount nominal J2000 RA",
      "type": "sexagesimalha",
      "daemon": "superwasp_telescope",
      "method": "report_status",
      "parameter": "ra"
    },
    {
      "key": "MNTDEC",
      "comment": "mount nominal J2000 Dec",
      "type": "sexagesimaldeg",
      "daemon": "superwasp_telescope",
      "method": "report_status",
      "parameter": "dec"
    },
    {
      "key": "MNTHA",
      "comment": "mount nominal HA",
      "type": "sexagesimalha",
      "daemon": "superwasp_telescope",
      "method": "report_status",
      "parameter": "ha"
    },
    {
      "key": "MNTRAD",
      "comment": "[deg] mount nominal J2000 RA",
      "type": "float5dp",
      "daemon": "superwasp_telescope",
      "method": "report_status",
      "parameter": "ra"
    },
    {
      "key": "MNTDECD",
      "comment": "[deg] mount nominal J2000 Dec",
      "type": "float5dp",
      "daemon": "superwasp_telescope",
      "method": "report_status",
      "parameter": "dec"
    },
    {
      "key": "MNTHAD",
      "comment": "[deg] mount nominal hour angle",
      "type": "float5dp",
      "daemon": "superwasp_telescope",
      "method": "report_status",
      "parameter": "ha"
    },
    {
      "key": "ALTITUDE",
      "comment": "[deg] mount nominal altitude",
      "type": "float5dp",
      "daemon": "superwasp_telescope",
      "method": "report_status",
      "parameter": "alt"
    },
    {
      "key": "AZIMUTH",
      "comment": "[deg] mount nominal azimuth",
      "type": "float5dp",
      "daemon": "superwasp_telescope",
      "method": "report_status",
      "parameter": "az"
    },
    {
      "key": "SITELAT",
      "comment": "telescope latitude (north)",
      "type": "sexagesimaldeg",
      "daemon": "superwasp_telescope",
      "method": "report_status",
      "parameter": "site_latitude"
    },
    {
      "key": "SITELONG",
      "comment": "telescope longitude (east)",
      "type": "sexagesimaldeg",
      "daemon": "superwasp_telescope",
      "method": "report_status",
      "parameter": "site_longitude"
    },
    {
      "key": "SITEELEV",
      "comment": "[m] telescope elevation",
      "type": "int",
      "daemon": "superwasp_telescope",
      "method": "report_status",
      "parameter": "site_elevation"
    },
    {
      "key": "MOONSEP",
      "comment": "[deg] nominal distance from moon",
      "type": "float1dp",
      "daemon": "superwasp_telescope",
      "method": "report_status",
      "parameter": "moon_separation"
    },
    {
      "key": "SUNSEP",
      "comment": "[deg] nominal distance from sun",
      "type": "float1dp",
      "daemon": "superwasp_telescope",
      "method": "report_status",
      "parameter": "sun_separation"
    },
    {
      "key": "LENSTEMP",
      "comment": "[deg c] lens temperature",
      "camera": "CAM1",
      "daemon": "superwasp_lensheater",
      "method": "report_status",
      "parameter": "temp_1",
      "type": "float1dp"
    },
    {
      "key": "LENSSET",
      "comment": "[deg c] lens temperature set point",
      "camera": "CAM1",
      "daemon": "superwasp_lensheater",
      "method": "report_status",
      "parameter": "set_1",
      "type": "float1dp"
    },
    {
      "key": "LENSTEMP",
      "comment": "[deg c] lens temperature",
      "camera": "CAM2",
      "daemon": "superwasp_lensheater",
      "method": "report_status",
      "parameter": "temp_2",
      "type": "float1dp"
    },
    {
      "key": "LENSSET",
      "comment": "[deg c] lens temperature set point",
      "camera": "CAM2",
      "daemon": "superwasp_lensheater",
      "method": "report_status",
      "parameter": "set_2",
      "type": "float1dp"
    },
    {
      "key": "LENSTEMP",
      "comment": "[deg c] lens temperature",
      "camera": "CAM3",
      "daemon": "superwasp_lensheater",
      "method": "report_status",
      "parameter": "temp_3",
      "type": "float1dp"
    },
    {
      "key": "LENSSET",
      "comment": "[deg c] lens temperature set point",
      "camera": "CAM3",
      "daemon": "superwasp_lensheater",
      "method": "report_status",
      "parameter": "set_3",
      "type": "float1dp"
    },
    {
      "key": "LENSTEMP",
      "comment": "[deg c] lens temperature",
      "camera": "CAM4",
      "daemon": "superwasp_lensheater",
      "method": "report_status",
      "parameter": "temp_4",
      "type": "float1dp"
    },
    {
      "key": "LENSSET",
      "comment": "[deg c] lens temperature set point",
      "camera": "CAM4",
      "daemon": "superwasp_lensheater",
      "method": "report_status",
      "parameter": "set_4",
      "type": "float1dp"
    }
  ]
}
