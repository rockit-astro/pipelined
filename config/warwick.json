{
  "daemon": "warwick_pipeline",
  "log_name": "pipelined@warwick",
  "control_machines": ["WarwickTCS"],
  "notify_frame_machines": ["WarwickTCS"],
  "environment_daemon": "warwick_environment",
  "environment_query_timeout": 1,
  "telescope_query_timeout": 1,
  "ops_daemon": "warwick_operations",
  "guiding_min_interval": 5,
  "cameras": {
    "QHY600M": {
      "worker_daemon": "warwick_pipeline_cam",
      "worker_processes": 4,
      "input_data_path": "/data/incoming",
      "output_data_path": "/data",
      "output_filename_add_camera_id": false,
      "output_filename_add_filter": true,
      "binning_card": "CAM-BIN",
      "image_region_card": "IMAG-RGN",
      "overscan_region_card": "BIAS-RGN",
      "platescale": 0.20,
      "object_minpix": 16,
      "preview_min_interval": 5,
      "preview_max_instances": 5,
      "preview_ds9_width": 680,
      "preview_ds9_height": 480,
      "preview_ds9_zoom": 0.0625,
      "preview_ds9_annotation_margin": 175,
      "preview_display_camera_id": false,
      "preview_display_filter": true,
      "hfd_grid_tiles_x": 5,
      "hfd_grid_tiles_y": 4,
      "wcs": {
        "scale_low": 0.18,
        "scale_high": 0.22,
        "timeout": 4.0,
        "tel_ra_card": "TELRA",
        "tel_dec_card": "TELDEC",
        "search_radius": 3
      }
    }
  },
    "environment_cards": [
    {
      "key": "EXTTEMP",
      "comment": "[deg c] temperature outside dome",
      "sensor": "vaisala",
      "parameter": "temperature",
      "type": "float1dp"
    },
    {
      "key": "EXTHUMD",
      "comment": "[%] humidity outside dome",
      "sensor": "vaisala",
      "parameter": "relative_humidity",
      "type": "float1dp"
    },
    {
      "key": "PRESSURE",
      "comment": "[hPa] air pressure",
      "sensor": "vaisala",
      "parameter": "pressure",
      "type": "float1dp"
    },
    {
      "key": "WINDSPD",
      "comment": "[m/s] wind speed outside dome",
      "sensor": "vaisala",
      "parameter": "wind_speed",
      "type": "float1dp"
    },
    {
      "key": "WINDGUST",
      "comment": "[m/s] wind gust speed outside dome",
      "sensor": "vaisala",
      "parameter": "wind_gust",
      "type": "float1dp"
    },
    {
      "key": "MEDWIND",
      "comment": "[m/s] median wind speed (last 20 min)",
      "sensor": "vaisala",
      "parameter": "median_wind_speed",
      "type": "float1dp"
    },
    {
      "key": "DEWDELTA",
      "comment": "[deg c] temperature above dew point",
      "sensor": "vaisala",
      "parameter": "dew_point_delta",
      "type": "float1dp"
    },
    {
      "key": "SKYTEMP",
      "comment": "[deg c] sky temperature",
      "sensor": "cloudwatcher",
      "parameter": "sky_temp",
      "type": "float1dp"
    },
    {
      "key": "SUNALT",
      "comment": "[deg] sun altitude",
      "sensor": "ephem",
      "parameter": "sun_alt",
      "type": "float1dp"
    },
    {
      "key": "MOONALT",
      "comment": "[deg] moon altitude",
      "sensor": "ephem",
      "parameter": "moon_alt",
      "type": "float1dp"
    },
    {
      "key": "MOONPCT",
      "comment": "[%] moon illumination",
      "sensor": "ephem",
      "parameter": "moon_percent_illumination",
      "type": "float1dp"
    }
  ],
  "telescope_cards": [
    {
      "key": "MNTSTATE",
      "comment": "telescope status",
      "type": "string",
      "daemon": "warwick_telescope",
      "method": "report_status",
      "parameter": "state_label"
    },
    {
      "key": "DOMESHST",
      "comment": "dome shutter status",
      "type": "string",
      "daemon": "warwick_dome",
      "method": "status",
      "parameter": "shutter_label"
    },
    {
      "key": "DOMEAZST",
      "comment": "dome azimuth status",
      "type": "string",
      "daemon": "warwick_dome",
      "method": "status",
      "parameter": "azimuth_status_label"
    },
    {
      "key": "DOMEAZ",
      "comment": "dome azimuth",
      "type": "float1dp",
      "daemon": "warwick_dome",
      "method": "status",
      "parameter": "azimuth"
    },
    {
      "key": "MNTRA",
      "comment": "mount nominal J2000 RA",
      "type": "sexagesimalha",
      "daemon": "warwick_telescope",
      "method": "report_status",
      "parameter": "ra"
    },
    {
      "key": "MNTDEC",
      "comment": "mount nominal J2000 Dec",
      "type": "sexagesimaldeg",
      "daemon": "warwick_telescope",
      "method": "report_status",
      "parameter": "dec"
    },
    {
      "key": "MNTHA",
      "comment": "mount nominal HA",
      "type": "sexagesimalha",
      "daemon": "warwick_telescope",
      "method": "report_status",
      "parameter": "ha"
    },
    {
      "key": "MNTRAD",
      "comment": "[deg] mount nominal J2000 RA",
      "type": "float5dp",
      "daemon": "warwick_telescope",
      "method": "report_status",
      "parameter": "ra"
    },
    {
      "key": "MNTDECD",
      "comment": "[deg] mount nominal J2000 Dec",
      "type": "float5dp",
      "daemon": "warwick_telescope",
      "method": "report_status",
      "parameter": "dec"
    },
    {
      "key": "MNTHAD",
      "comment": "[deg] mount nominal hour angle",
      "type": "float5dp",
      "daemon": "warwick_telescope",
      "method": "report_status",
      "parameter": "ha"
    },
    {
      "key": "ALTITUDE",
      "comment": "[deg] mount nominal altitude",
      "type": "float5dp",
      "daemon": "warwick_telescope",
      "method": "report_status",
      "parameter": "alt"
    },
    {
      "key": "AZIMUTH",
      "comment": "[deg] mount nominal azimuth",
      "type": "float5dp",
      "daemon": "warwick_telescope",
      "method": "report_status",
      "parameter": "az"
    },
    {
      "key": "TELFOC",
      "comment": "[steps] nominal telescope focus",
      "type": "int",
      "daemon": "warwick_focuser",
      "method": "report_status",
      "parameter": "current_steps"
    },
    {
      "key": "SITELAT",
      "comment": "telescope latitude (north)",
      "type": "sexagesimaldeg",
      "daemon": "warwick_telescope",
      "method": "report_status",
      "parameter": "site_latitude"
    },
    {
      "key": "SITELONG",
      "comment": "telescope longitude (east)",
      "type": "sexagesimaldeg",
      "daemon": "warwick_telescope",
      "method": "report_status",
      "parameter": "site_longitude"
    },
    {
      "key": "SITEELEV",
      "comment": "[m] telescope elevation",
      "type": "int",
      "daemon": "warwick_telescope",
      "method": "report_status",
      "parameter": "site_elevation"
    },
    {
      "key": "MOONSEP",
      "comment": "[deg] nominal distance from moon",
      "type": "float1dp",
      "daemon": "warwick_telescope",
      "method": "report_status",
      "parameter": "moon_separation"
    },
    {
      "key": "SUNSEP",
      "comment": "[deg] nominal distance from sun",
      "type": "float1dp",
      "daemon": "warwick_telescope",
      "method": "report_status",
      "parameter": "sun_separation"
    },
    {
      "key": "FILTER",
      "comment": "filter in light path",
      "type": "string",
      "daemon": "warwick_filterwheel",
      "method": "report_status",
      "parameter": "filter"
    }
  ]
}
