{
  "daemon": "clasp_pipeline_2",
  "log_name": "pipelined@clasp2",
  "control_machines": ["CLASPTCS", "CLASPDAS2"],
  "notify_frame_machines": ["CLASPDAS2"],
  "environment_daemon": "observatory_environment",
  "environment_query_timeout": 1,
  "telescope_query_timeout": 1,
  "ops_daemon": "clasp_operations",
  "incoming_data_path": "/data/incoming",
  "data_root_path": "/data",
  "dashboard_output_path": "/mnt/dashboard-generated/",
  "cameras": {
    "CAM2": {
      "wcs_scale_low": 0.95,
      "wcs_scale_high": 1.0,
      "wcs_timeout": 4.0,
      "wcs_search_ra_card": "TELRA",
      "wcs_search_dec_card": "TELDEC",
      "wcs_search_radius": 1.75,
      "ccd_bin_card": "CCD-XBIN",
      "image_region_card": "IMAG-RGN",
      "overscan_region_card": "BIAS-RGN",
      "platescale": 0.988,
      "object_minpix": 16,
      "preview_ds9_width": 680,
      "preview_ds9_height": 480,
      "preview_ds9_zoom": 0.0625,
      "preview_ds9_annotation_margin": 175,
      "dashboard_flip_vertical": true,
      "dashboard_flip_horizontal": false,
      "dashboard_min_threshold": 5,
      "dashboard_max_threshold": 95,
      "dashboard_thumb_size": 1024,
      "dashboard_clip_size": 1024,
      "dashboard_max_cadence": 30,
      "hfd_grid_tiles_x": 5,
      "hfd_grid_tiles_y": 4
    }
  },
  "environment_cards": [
    {
      "key": "EXTTEMP",
      "comment": "[deg c] temperature outside dome",
      "sensor": "w1m_vaisala",
      "parameter": "temperature",
      "type": "float1dp"
    },
    {
      "key": "EXTHUMD",
      "comment": "[%] humidity outside dome",
      "sensor": "w1m_vaisala",
      "parameter": "relative_humidity",
      "type": "float1dp"
    },
    {
      "key": "PRESSURE",
      "comment": "[hPa] air pressure",
      "sensor": "w1m_vaisala",
      "parameter": "pressure",
      "type": "float1dp"
    },
    {
      "key": "WINDSPD",
      "comment": "[m/s] wind speed outside dome",
      "sensor": "w1m_vaisala",
      "parameter": "wind_speed",
      "type": "float1dp"
    },
    {
      "key": "WINDGUST",
      "comment": "[m/s] wind gust speed outside dome",
      "sensor": "w1m_vaisala",
      "parameter": "wind_gust",
      "type": "float1dp"
    },
    {
      "key": "MEDWIND",
      "comment": "[m/s] median wind speed (last 20 min)",
      "sensor": "w1m_vaisala",
      "parameter": "median_wind_speed",
      "type": "float1dp"
    },
    {
      "key": "SKYCLAR",
      "comment": "[deg c] sky clarity",
      "sensor": "superwasp_aurora",
      "parameter": "clarity",
      "type": "float1dp"
    },
    {
      "key": "DEWDELTA",
      "comment": "[deg c] temperature above dew point",
      "sensor": "w1m_vaisala",
      "parameter": "dew_point_delta",
      "type": "float1dp"
    },
    {
      "key": "TNGDIMM",
      "comment": "[arcsec] TNG DIMM seeing measurement",
      "sensor": "tng",
      "parameter": "seeing",
      "type": "float2dp"
    },
    {
      "key": "INGDIMM",
      "comment": "[arcsec] ING RoboDIMM seeing measurement",
      "sensor": "robodimm",
      "parameter": "seeing",
      "type": "float2dp"
    },
    {
      "key": "TNGDUST",
      "comment": "[ug/m3] TNG Dust measurement",
      "sensor": "tng",
      "parameter": "dust",
      "type": "float1dp"
    }
  ],
  "telescope_cards": [
    {
      "key": "MNTSWVER",
      "comment": "mount server software version",
      "type": "string",
      "daemon": "clasp_telescope",
      "method": "report_status",
      "parameter": "software_version"
    },
    {
      "key": "MNTSTATE",
      "comment": "telescope status",
      "type": "string",
      "daemon": "clasp_telescope",
      "method": "report_status",
      "parameter": "state_label"
    },
    {
      "key": "DOMENORT",
      "comment": "dome north shutter status",
      "type": "string",
      "daemon": "clasp_dome",
      "method": "status",
      "parameter": "shutter_a_label"
    },
    {
      "key": "DOMESOUT",
      "comment": "dome south shutter status",
      "type": "string",
      "daemon": "clasp_dome",
      "method": "status",
      "parameter": "shutter_b_label"
    },
    {
      "key": "MNTRA",
      "comment": "mount nominal J2000 RA",
      "type": "sexagesimalha",
      "daemon": "clasp_telescope",
      "method": "report_status",
      "parameter": "ra"
    },
    {
      "key": "MNTDEC",
      "comment": "mount nominal J2000 Dec",
      "type": "sexagesimaldeg",
      "daemon": "clasp_telescope",
      "method": "report_status",
      "parameter": "dec"
    },
    {
      "key": "MNTHA",
      "comment": "mount nominal HA",
      "type": "sexagesimalha",
      "daemon": "clasp_telescope",
      "method": "report_status",
      "parameter": "ha"
    },
    {
      "key": "MNTRAD",
      "comment": "[deg] mount nominal J2000 RA",
      "type": "float5dp",
      "daemon": "clasp_telescope",
      "method": "report_status",
      "parameter": "ra"
    },
    {
      "key": "MNTDECD",
      "comment": "[deg] mount nominal J2000 Dec",
      "type": "float5dp",
      "daemon": "clasp_telescope",
      "method": "report_status",
      "parameter": "dec"
    },
    {
      "key": "MNTHAD",
      "comment": "[deg] mount nominal hour angle",
      "type": "float5dp",
      "daemon": "clasp_telescope",
      "method": "report_status",
      "parameter": "ha"
    },
    {
      "key": "ALTITUDE",
      "comment": "[deg] mount nominal altitude",
      "type": "float5dp",
      "daemon": "clasp_telescope",
      "method": "report_status",
      "parameter": "alt"
    },
    {
      "key": "AZIMUTH",
      "comment": "[deg] mount nominal azimuth",
      "type": "float5dp",
      "daemon": "clasp_telescope",
      "method": "report_status",
      "parameter": "az"
    },
    {
      "key": "TELFOC",
      "comment": "[steps] nominal telescope focus",
      "camera": "CAM1",
      "type": "int",
      "daemon": "clasp_focus",
      "method": "report_status",
      "parameter": "current_steps_1"
    },
    {
      "key": "TELFOC",
      "comment": "[steps] nominal telescope focus",
      "camera": "CAM2",
      "type": "int",
      "daemon": "clasp_focus",
      "method": "report_status",
      "parameter": "current_steps_2"
    },
    {
      "key": "SITELAT",
      "comment": "telescope latitude (north)",
      "type": "sexagesimaldeg",
      "daemon": "clasp_telescope",
      "method": "report_status",
      "parameter": "site_latitude"
    },
    {
      "key": "SITELONG",
      "comment": "telescope longitude (east)",
      "type": "sexagesimaldeg",
      "daemon": "clasp_telescope",
      "method": "report_status",
      "parameter": "site_longitude"
    },
    {
      "key": "SITEELEV",
      "comment": "[m] telescope elevation",
      "type": "int",
      "daemon": "clasp_telescope",
      "method": "report_status",
      "parameter": "site_elevation"
    },
    {
      "key": "MOONSEP",
      "comment": "[deg] nominal distance from moon",
      "type": "float1dp",
      "daemon": "clasp_telescope",
      "method": "report_status",
      "parameter": "moon_separation"
    },
    {
      "key": "SUNSEP",
      "comment": "[deg] nominal distance from sun",
      "type": "float1dp",
      "daemon": "clasp_telescope",
      "method": "report_status",
      "parameter": "sun_separation"
    }
  ]
}
